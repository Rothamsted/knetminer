		<description>Coronavirus and drugs</description>
		<term>"ascorbic acid" OR favipiravir OR chloroquine OR leflunomide OR Remdesivir OR methylprednisolone OR Tocilizumab OR Sarilumab</term>
	</query>  
	<query>
